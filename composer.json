{
    "name": "softstone/talent",
    "type": "project",
    "description": "软通人才库项目",
    "keywords": [
        "php",
        "swoole",
        "coroutine",
        "simple",
        "simps"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.1",
        "ext-swoole": ">=4.4",
        "simple-swoole/simps": "~1.0.0",
        "simple-swoole/db": "~1.0.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dotenv": "^5.1",
        "monolog/monolog": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "bin": [
        "bin/simps.php"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "swoole/ide-helper": "4.4.18",
        "ukko/phpredis-phpdoc": "5.0.1-beta6",
        "phpunit/phpunit": "^9.3"
    }
}
